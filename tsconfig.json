    "resolveJsonModule": true,
    "types": ["vite/client", "kakao.maps.d.ts"],
    "noImplicitAny": false,
    "noUnusedLocals": false
  },
  "include": ["src", "swiper.d.ts", "src/globals.d.ts"],
  "exclude": ["node_modules"]
}
